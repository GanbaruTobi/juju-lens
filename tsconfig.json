{
  "extends": "@quasar/app/tsconfig-preset",
  "exclude": [
    "src/statics",
    "*gitignore*",
    "src-tauri"
  ],
  "compilerOptions": {
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "store*": [
        "src/store*"
      ],
      "utils*": [
        "src/utils*"
      ]
    }
  }
}