{
  "build": {
    "distDir": "../dist/spa",
    "devPath": "http://127.0.0.1:8080",
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "withGlobalTauri": true
  },
  "ctx": {},
  "tauri": {
    "embeddedServer": {
      "active": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.katharostech.juju.JujuLens",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "Â© Katharos Technology & Contributors",
      "category": "DeveloperTool",
      "shortDescription": "The Juju Lens Juju administration tool",
      "longDescription": "The Juju Lens Juju administration tool",
      "osx": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false
      },
      "exceptionDomain": ""
    },
    "allowlist": {
      "all": true,
      "event": true,
      "execute": false,
      "listFiles": false,
      "open": false,
      "readBinaryFile": false,
      "readTextFile": false,
      "setTitle": false,
      "writeFile": false,
      "notification": true
    },
    "window": {
      "title": "Juju Lens",
      "width": 800,
      "height": 600,
      "resizable": true,
      "fullscreen": false
    },
    "security": {
      "csp": "default-src data: ws: localhost:* 127.0.0.1:* 'unsafe-inline' 'unsafe-eval'; img-src localhost:* 127.0.0.1:* data: api.jujucharms.com"
    },
    "inliner": {
      "active": true
    }
  }
}
